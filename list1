x=int(input("enter any no "))
while (x>0):
    n=x
    list = []
    tt = tuple()
    while(n>0):
        a=n%10
        list.append(a)
        n=n//10
        tt = tt+(n,)
    s=0
    for i in range(len(list)):
        s=s+list[i]**list[i]
    print("Required result is", s)
    print("Tuple is")
    print(tt)
    x=int(input("enter any no "))
print("OK Bye")   
